<%= render 'user_groups_header' %>
	<div class="panel panel-default">
	<div class="panel-heading"><h3>User Groups</h3></div>
	<div class="panel-body">
		<%= bootstrap_form_for(@user_group, layout: :horizontal) do |f| %>
			<%= f.static_control :user_group %>
						
			<%= f.text_field :group_name %>
			
			</div>
			</div>			
			<div class="row"><%= link_to "Back", user_groups_path, class: "btn btn-default" %><div class="pull-right"><%= f.submit "Submit", class: "btn btn-large btn-primary" %></div></div>
		<% end %>
	</div>
</div>
