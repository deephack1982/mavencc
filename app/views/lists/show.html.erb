<% provide(:title, 'Lists') %>
<h1>Lists</h1>
<%= render 'list_tab' %>
<div class="col-md-8">
	<%= bootstrap_form_for(@list, style: :horizontal) do |f| %>
		<%= f.static_control :list_id %>
		<%= f.static_control :list_name %>
		<%= f.static_control :list_description %>
		<%= f.static_control :active %>
		<%= f.static_control :campaign_name %>
		<%= f.static_control :reset_time %>
	<% end %>
	<div class="row"><%= link_to "Back", lists_path, class: "btn btn-default" %><div class="pull-right"><%= link_to "Edit", edit_list_path, class: "btn btn-info" %></div></div><p>
</div>
</div>
</div>
