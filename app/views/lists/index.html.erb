<%= render 'lists_header' %>
	<%= will_paginate %>
	<div class="table-responsive">
		<small><%= page_entries_info @lists %></small>
	<table class="table table-striped table-hover">
		<tr><th <%= sortable "list_id" %></th><th <%= sortable "list_name" %></th><th <%= sortable "active" %></th><th <%= sortable "campaign_name" %></th><th <%= sortable "Leads" %></th><th <%= sortable "Last called" %></th><th>Actions</th></tr>
		<% @lists.each do |l| %>
		<tr><td><span class="label label-info"><%= link_to l.list_id, l %></span></td><td><%= l.list_name %></td><td><%= l.active %></td><td><%= l.campaign_name %></td><td><%= l.leads.count %></td><td><%= l.list_lastcalldate.to_date %></td><td><div class="btn-group"><%= link_to 'Delete', list_path(l), method: :delete, data: { confirm: "This will permanantly remove this list, are you sure?"}, class: "btn-sm btn-danger", role: "button" %><%= link_to "Edit", edit_list_path(l), class: "btn-sm btn-warning" %><%= link_to "Show", list_path(l), class: "btn-sm btn-info" %></div></tr>
		<% end %>
		</table>
		</div>
		<%= will_paginate %>
	</div>
	</div>
