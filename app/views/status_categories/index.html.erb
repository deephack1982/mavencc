<%= render 'admin/admin_header' %>
<div class="panel panel-default">
<div class="panel-heading"><h3>Status Categories</h3></div>
<div class="panel-body">
	<div class="table-responsive">
	<table class="table table-striped table-hover">
	<tr><th <%= sortable "id","Id" %></th><th <%= sortable "vsc_id","Category" %></th><th <%= sortable "vsc_name","Category Name" %></th></tr>
	<% @status_categories.each do |s| %>
	<tr><td><%= link_to s.id, status_category_path(s), class: "label label-info" %></td><td><%= s.vsc_id %></td><td><%= s.vsc_name %></td></tr>
	<% end %>
	</table>
	</div>
</div>
</div>
</div>
