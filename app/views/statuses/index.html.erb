<%= render 'admin/admin_header' %>
<div class="panel panel-default">
<div class="panel-heading"><h3>Statuses</h3></div>
<div class="panel-body">
	<div class="table-responsive">
	<table class="table table-striped table-hover">
	<tr><th <%= sortable "id","Id" %></th><th <%= sortable "status","Status" %></th><th <%= sortable "status_name","Status Name" %></th><th <%= sortable "category","Category" %></th></tr>
	<% @statuses.each do |s| %>
	<tr><td><span class="label label-info"><%= link_to s.id, status_path(s) %></span></td><td><%= s.status %></td><td><%= s.status_name %></td><td><%= s.category %></td></tr>
	<% end %>
	</table>
	</div>
</div>
</div>
</div>
