<%= render 'filter_header' %>
<div class="panel panel-default">
<div class="panel-heading"><h3><%= "#{@filter.lead_filter_id} - #{@filter.lead_filter_name}" %></h3></div>
<div class="panel-body">
	<%= bootstrap_form_for(@filter) do |f| %>
		<%= f.static_control :lead_filter_id %>
		<%= f.static_control :lead_filter_name %>
		<%= f.static_control :lead_filter_comments %>
		<%= f.static_control :lead_filter_sql %>
		<%= f.static_control :group_name do %>
			<%= link_to @filter.user_group, user_group_path(@filter.user_group_id), class: "label label-default"  %>
		<% end %>
	<% end %>
		</div>
	</div>
		<div class="row"><%= link_to "Back", filters_path, class: "btn btn-default" %><span class="pull-right"><%= link_to "Edit", edit_filter_path, class: "btn btn-info" %></span></div>
	</div>
	</div>
</div>
